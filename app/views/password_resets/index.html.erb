<h1>Listing password_resets</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @password_resets.each do |password_reset| %>
      <tr>
        <td><%= link_to 'Show', password_reset %></td>
        <td><%= link_to 'Edit', edit_password_reset_path(password_reset) %></td>
        <td><%= link_to 'Destroy', password_reset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

